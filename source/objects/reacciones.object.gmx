            <kind>1</kind>
            <string>var query = "SELECT r.id_reaccion, rm1.cant_mol, m1.s_formula, rm2.cant_mol, m2.s_formula, 
            rm3.cant_mol, m3.s_formula, tr.desc_tipo_reaccion
            FROM Molecula m1, Molecula m2, Molecula m3, reaccion_molecula rm1, reaccion_molecula rm2, 
            reaccion_molecula rm3, Reaccion r, rol_molecula rol1, rol_molecula rol2, rol_molecula rol3,
            TipoReaccion tr
            WHERE m1.id_molecula = rm1.id_molecula AND r.id_reaccion = rm1.id_reaccion 
            AND m2.id_molecula = rm2.id_molecula AND r.id_reaccion = rm2.id_reaccion 
            AND rm1.id_rol = rol1.id_rol AND rol1.desc_rol = 'REACTIVO' 
            AND rm2.id_rol = rol2.id_rol AND rol2.desc_rol = 'REACTIVO' 
            AND NOT m1.s_formula = m2.s_formula AND tr.id_tipo_reaccion = r.id_tipo_reaccion
            AND rol3.desc_rol = 'PRODUCTO' AND rol3.id_rol = rm3.id_rol
            AND rm3.id_reaccion = r.id_reaccion AND rm3.id_molecula = m3.id_molecula
            AND r.b_descubierta=1 GROUP BY r.id_reaccion ORDER BY r.id_reaccion;";
rr = runQuery(query, true);
re = ds_list_create();

for (var j=0; j&lt;ds_list_size(rr)/8; j++) {
    re[|j] = " "+rr[|8*j+1]+rr[|8*j+2]+" + "+rr[|8*j+3]+rr[|8*j+4]+" --&gt; "+rr[|8*j+5]+rr[|8*j+6]+" -- "+rr[|8*j+7];
}
ds_list_destroy(rr);
_y = y;
seleccionado = false;
max_y = y;
recuadro = surface_create(960, 480);

</string>
          </argument>
        </arguments>
      </action>
